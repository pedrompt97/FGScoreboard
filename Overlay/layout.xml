  <button type="reset" x="435" y="58" width="40" height="23" tooltip="Revive Team 2" id="revive2" reset="t2Img1,t2Img2,t2Img3,t2Img5,t2Img5,t2Img6,t2Img7">Revive</button>
  <button type="reset" x="435" y="98" width="40" height="23" tooltip="Clear Team 2 fields" id="cleart2" reset="t2Img1,t2Img2,t2Img3,t2Img4,t2Img5,t2Img6,t2Img7,a2Img1,a2Img2,a2Img3,a2Img4,a2Img5,a2Img6,a2Img7">Clear</button>
	
