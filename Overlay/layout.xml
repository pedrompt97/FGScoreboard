<!DOCTYPE StreamControlLayout>
<layout width="450" height="260" tabbed="1">
<tab name="Scoreboard">
  <lineEdit id="pName1" x="10" y="20" width="200" height="20" dataSet="players.csv" >Player 1</lineEdit>
  <lineEdit id="pCountry1" x="220" y="20" width="50" height="20" dataSet="players.csv" dataField="2" master="pName1" >Country</lineEdit>
  <lineEdit id="pName2" x="10" y="60" width="200" height="20" dataSet="players.csv" >Player 2</lineEdit>
  <lineEdit id="pCountry2" x="220" y="60" width="50" height="20" dataSet="players.csv" dataField="2" master="pName2" >Country</lineEdit>
  <spinBox id="pScore1" x="280" y="20" width="50" height="22" maximum="999" />
  <spinBox id="pScore2" x="280" y="60" width="50" height="22" maximum="999" />
  
  <button type="reset" x="335" y="20" width="40" height="23" tooltip="Clear Player 1 fields" id="clear1" reset="pName1,pCountry1">Clear</button>
  <button type="reset" x="380" y="20" width="40" height="23" tooltip="Reset the Scores" id="reset" reset="pScore1,pScore2">Reset</button>
  <button type="reset" x="335" y="60" width="40" height="23" tooltip="Clear Player 2 fields" id="clear2" reset="pName2,pCountry2">Clear</button>
  <button type="swap" x="380" y="60" width="40" height="23" tooltip="Swap the Scores" id="swap" swapSet1="pName1,pScore1,pCountry1" swapSet2="pName2,pScore2,pCountry2">Swap</button>
  
  <label x="10" y="105" width="46" height="13">Label</label>
  <comboBox id="mText3" x="65" y="100" width="355" height="22" editable="1">	
		<comboItem> </comboItem>
		<comboItem>Casuals</comboItem>
		<comboItem>Swiss (Round 1)</comboItem>
		<comboItem>Swiss (Round 2)</comboItem>
		<comboItem>Swiss (Round 3)</comboItem>
		<comboItem>Swiss (Round 4)</comboItem>
		<comboItem>Swiss (Round 5)</comboItem>
		<comboItem>Swiss (Round 6)</comboItem>
		<comboItem>Swiss (Round 7)</comboItem>
		<comboItem>Exhibition FT5</comboItem>
		<comboItem>Exhibition FT10</comboItem>
		<comboItem>Team Exhibition FT18</comboItem>
		<comboItem>Winners Bracket</comboItem>
		<comboItem>Losers Bracket</comboItem>
		<comboItem>Top 8 Winners</comboItem>
		<comboItem>Top 8 Losers</comboItem>
		<comboItem>Top 8</comboItem>
		<comboItem>Winners Final</comboItem>
		<comboItem>Winners Semis</comboItem>	
		<comboItem>Losers Semis</comboItem>
		<comboItem>Losers Final</comboItem>
		<comboItem>Grand Finals</comboItem>
		<comboItem>Grand Finals (Reset)</comboItem>
	</comboBox>
</tab>


<tab name="Commentators">
  <label x="10" y="15" width="60" height="13">Left Caster</label>
  <label x="10" y="75" width="60" height="13">Right Caster</label>
	<lineEdit id="mText1" x="85" y="10" width="280" height="22" dataSet="casters.csv">Commentator 1</lineEdit>
  <lineEdit id="mText4" x="85" y="40" width="280" height="22" dataSet="casters.csv" dataField="2" master="mText1">Twitter 1</lineEdit>
	<lineEdit id="mText2" x="85" y="70" width="280" height="22" dataSet="casters.csv">Commentator 2</lineEdit>
  <lineEdit id="mText5" x="85" y="100" width="280" height="22" dataSet="casters.csv" dataField="2" master="mText2">Twitter 2</lineEdit>
	<button type="reset" x="380" y="10" width="40" height="23" tooltip="Clear Commentator 1 fields" id="clear4" reset="mText4,mText1">Clear</button>
  <button type="swap" x="380" y="40" width="40" height="23" tooltip="Swap the Casters" id="swap2" swapSet1="mText1,mText4" swapSet2="mText2,mText5">Swap</button>
	<button type="reset" x="380" y="70" width="40" height="23" tooltip="Clear Commentator 2 fields" id="clear3" reset="mText2,mText5">Clear</button>
</tab>

